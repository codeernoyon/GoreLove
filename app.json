{
  "expo": {
    "name": "deliveryApp",
    "slug": "deliveryApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icons/blood.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/icons/blood.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
